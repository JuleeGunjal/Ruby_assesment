<div class="col-4" style="margin : 0 auto; padding: 10px">
  <h1>All Students data</h1></br>
</div>
<table class="table table-striped">
  <thead class="thead-light">
    <tr>
      <td>ID</td>
      <td>Name</td>
      <td>Email</td>
      <td>Semister</td>
      <td>Course</td>
    </tr>
  </thead>
  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%=student.id%></td>
        <td><%= student.name %></td>
        <td><%= student.email %></td> 
        <td><%= student.semister %></td>
        <td><%= student.course %></td>
        <td><%= link_to 'show', student%></td>
      </tr>
   <% end %>
  </tbody>
</table>
<div class="col-2" style="margin : 0 auto; padding: 10px">
  <%= link_to 'Add new', new_student_path, class: "btn btn-success" %>
</div>