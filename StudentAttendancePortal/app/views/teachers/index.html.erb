<h1>All Teachers data</h1></br></br>
<table class="table table-striped">
<thead class="thead-light">
<tr>
<td>ID</td>
<td>Name</td>
<td>Email</td>
<td>Mobile number</td>
</tr>
</thead>
<tbody>
<% @teachers.each do |teacher| %>
<tr>
  <td><%=teacher.id%></td>
  <td><%= teacher.name %></td>
  <td><%= teacher.email %></td> 
  <td><%= teacher.mobile_no %></td>
  <td><%= link_to 'show', teacher%></td>
</tr>
<% end %>
</tbody>
</table>

<%= link_to 'Add new', new_teacher_path%>
